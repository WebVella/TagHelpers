					Form
				</div>
				<div class="card-body">
					<wv-grid bordered="true" small="true" columns="@gridColumns" has-tfoot="false">
						@foreach (var record in gridRecords)
						{
							<wv-grid-row>
								<wv-grid-column>@Html.Raw(record)</wv-grid-column>
							</wv-grid-row>
						}

					</wv-grid>
				</div>
			</div>
		</div>
