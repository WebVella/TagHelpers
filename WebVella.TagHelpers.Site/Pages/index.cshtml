	<div class="row">
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					Icon
				</div>
				<div class="card-body">
					<wv-alert title="section title" content="This is the alert text" icon-class="fa fa-cog" type="Success"></wv-alert>
				</div>
			</div>
		</div>
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					No Icon
				</div>
				<div class="card-body">
					<wv-alert title="section title" content="This is the alert text" type="Success"></wv-alert>
				</div>
			</div>
		</div>
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					No Title or content
				</div>
				<div class="card-body">
					<wv-alert title="" content="" icon-class="fas fa-fw fa-check" type="Success"></wv-alert>
				</div>
			</div>
		</div>
	</div>

	<h2>wv-validation</h2>

	<div class="row">
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					Only errors
				</div>
				<div class="card-body">
					<wv-validation errors="valErrors"></wv-validation>
				</div>
			</div>
		</div>
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					Only message
				</div>
				<div class="card-body">
					<wv-validation message="Validation Error!"></wv-validation>
				</div>
			</div>
		</div>
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					Message and errors
				</div>
				<div class="card-body">
					<wv-validation errors="valErrors" message="Validation Error!"></wv-validation>
				</div>
			</div>
		</div>
		<div class="col-3">
			<div class="card mt-3 mb-3">
				<div class="card-header">
					With icon and field names showing
				</div>
				<div class="card-body">
					<wv-validation errors="valErrors" message="Validation Error!" show-icon="true" show-key="true"></wv-validation>
				</div>
			</div>
		</div>
	</div>

